{
  "name": "@deadlock-mods/auth",
  "version": "1.0.2",
  "type": "module",
  "scripts": {
    "build": "tsr generate && vite build",
    "check-types": "tsc --noEmit --skipLibCheck",
    "start": "dotenv -e ../../.env -- bun run --preload src/instrument.ts src/index.ts",
    "dev:server": "dotenv -e ../../.env -- bun run --watch --preload src/instrument.ts src/index.ts",
    "dev:client": "vite build --watch",
    "dev": "concurrently \"pnpm run dev:server\" \"pnpm run dev:client\""
  },
  "dependencies": {
    "@better-fetch/fetch": "^1.1.18",
    "@deadlock-mods/common": "workspace:*",
    "@deadlock-mods/database": "workspace:*",
    "@deadlock-mods/feature-flags": "workspace:*",
    "@deadlock-mods/instrumentation": "workspace:*",
    "@deadlock-mods/logging": "workspace:*",
    "@deadlock-mods/shared": "workspace:*",
    "@deadlock-mods/ui": "workspace:*",
    "@hono/prometheus": "^1.0.2",
    "@hono/sentry": "^1.2.2",
    "@sentry/node": "catalog:",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query": "^5.62.11",
    "@tanstack/react-router": "^1.90.0",
    "@tanstack/router-cli": "^1.90.0",
    "better-auth": "catalog:",
    "dotenv-cli": "^7.4.4",
    "genversion": "^3.2.0",
    "hono": "^4.10.3",
    "neverthrow": "^8.2.0",
    "prom-client": "^15.1.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.68.0",
    "reflect-metadata": "^0.2.2",
    "tailwindcss": "^4.1.18",
    "tsyringe": "^4.10.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@deadlock-mods/typescript-config": "workspace:*",
    "@hookform/resolvers": "^5.2.2",
    "@types/bun": "^1.3.4",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.2.1",
    "vite": "^6.0.5"
  }
}
