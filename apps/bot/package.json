{
  "name": "@deadlock-mods/bot",
  "version": "1.1.0",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "tsc && tsc-alias",
    "check-types": "tsc --noEmit --skipLibCheck",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
    "dev": "dotenv -e ../../.env -- bun run --preload src/instrument.ts --watch src/index.ts",
    "start": "bun run --preload src/instrument.ts src/index.ts",
    "test:support": "dotenv -e ../../.env -- bun run --preload src/instrument.ts src/scripts/test-support.ts",
    "test:docs-sync": "dotenv -e ../../.env -- bun run --preload src/instrument.ts src/scripts/test-documentation-sync.ts",
    "version:generate": "genversion -f -p ./package.json --es6 src/version.ts"
  },
  "dependencies": {
    "@deadlock-mods/common": "workspace:*",
    "@deadlock-mods/database": "workspace:*",
    "@deadlock-mods/instrumentation": "workspace:*",
    "@deadlock-mods/logging": "workspace:*",
    "@deadlock-mods/queue": "workspace:*",
    "@deadlock-mods/shared": "workspace:*",
    "@deadlock-mods/vpk-parser": "workspace:*",
    "@discordjs/rest": "^2.6.0",
    "@langchain/community": "^1.0.0",
    "@langchain/core": "^1.0.1",
    "@langchain/langgraph": "^1.0.1",
    "@langchain/openai": "^1.0.0",
    "@langchain/textsplitters": "^1.0.0",
    "@langfuse/client": "^4.2.1",
    "@langfuse/core": "^4.2.1",
    "@langfuse/langchain": "^4.2.1",
    "@langfuse/otel": "^4.2.1",
    "@langfuse/tracing": "^4.2.1",
    "@opentelemetry/auto-instrumentations-node": "^0.64.1",
    "@opentelemetry/core": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.206.0",
    "@sapphire/discord.js-utilities": "^7.3.3",
    "@sapphire/framework": "^5.3.6",
    "@sentry/node": "catalog:",
    "@sentry/opentelemetry": "^10.15.0",
    "cache-manager": "catalog:",
    "cacheable": "catalog:",
    "discord-api-types": "^0.38.26",
    "discord.js": "^14.22.1",
    "genversion": "^3.2.0",
    "ioredis": "^5.8.0",
    "langchain": "^1.0.1",
    "lodash": "^4.17.21",
    "lzutf8": "^0.6.3",
    "neverthrow": "^8.2.0",
    "puppeteer": "^24.22.0",
    "xxhash-wasm": "^1.1.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@deadlock-mods/typescript-config": "workspace:*",
    "@types/bun": "^1.3.0",
    "@types/lodash": "^4.17.20",
    "@types/pg": "^8.15.5",
    "typescript": "catalog:"
  }
}
