{
  "name": "@deadlock-mods/database",
  "version": "1.8.0",
  "exports": {
    ".": "./src/client.ts"
  },
  "scripts": {
    "dev": "pnpm db:studio",
    "db:migrate": "dotenv -e ../../.env -- drizzle-kit migrate",
    "db:generate": "dotenv -e ../../.env -- drizzle-kit generate",
    "db:push": "dotenv -e ../../.env -- drizzle-kit push",
    "db:studio": "dotenv -e ../../.env -- drizzle-kit studio",
    "db:seed": "dotenv -e ../../.env -- bun run src/seed.ts"
  },
  "dependencies": {
    "@deadlock-mods/common": "workspace:*",
    "@deadlock-mods/logging": "workspace:*",
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.42.0",
    "neverthrow": "^8.2.0",
    "pg": "^8.16.3",
    "typeid-js": "^1.2.0"
  },
  "devDependencies": {
    "@deadlock-mods/typescript-config": "workspace:*",
    "@types/node": "^20.19.17",
    "@types/pg": "^8.15.5",
    "drizzle-kit": "^0.31.4"
  }
}
