{
  "name": "@deadlock-mods/kv-parser-rs",
  "version": "0.2.0",
  "description": "High-performance KeyValues (VDF) parser with AST support using native Rust implementation via Bun FFI",
  "exports": {
    ".": "./src/index.ts"
  },
  "files": [
    "src/",
    "target/release/libkv_parser.*",
    "target/release/kv_parser.*",
    "README.md"
  ],
  "scripts": {
    "build": "pnpm run generate-types && cargo build --release",
    "build:debug": "pnpm run generate-types && cargo build",
    "generate-types": "cargo test --lib --quiet || node -e \"process.exit(0)\"",
    "test": "pnpm run \"/^test:.*/\"",
    "test:ts": "bun test",
    "test:rust": "cargo test",
    "clean": "cargo clean && rm -rf src/generated/*.ts",
    "check": "cargo check",
    "prepublishOnly": "pnpm run build",
    "pretest": "pnpm run build"
  },
  "keywords": [
    "keyvalues",
    "vdf",
    "valve",
    "parser",
    "deadlock",
    "ast",
    "ffi",
    "rust",
    "native",
    "performance"
  ],
  "author": "stormix <contact@stormix.co>",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/deadlock-mod-manager/deadlock-mod-manager",
    "directory": "packages/kv-parser-rs"
  },
  "engines": {
    "bun": ">=1.3.0"
  },
  "devDependencies": {
    "@deadlock-mods/typescript-config": "workspace:*",
    "@types/bun": "^1.3.0",
    "@types/node": "^24.5.2",
    "bun-types": "^1.3.0"
  },
  "peerDependencies": {
    "bun": ">=1.3.0"
  }
}
